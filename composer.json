{
    "name": "omisai/vies-soap",
    "description": "PHP package to check Vat number through SOAP API of VIES",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omisai\\ViesSoap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "homepage": "https://github.com/omisai-tech/php-vies-soap",
    "authors": [
        {
            "name": "SÃ¡ndor Gergely Bornemisza",
            "email": "bornemisza@omisai.com",
            "homepage": "https://omisai.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^4.3"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php": "^8.1",
        "ext-soap": "*"
    },
    "scripts": {
        "test": "./vendor/bin/pest --colors=always",
        "compile": "php compiler.php"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}